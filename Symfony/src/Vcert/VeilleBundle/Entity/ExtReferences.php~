<?php

namespace Vcert\VeilleBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Vcert\VeilleBundle\Entity\ExtReferences
 */
class ExtReferences
{
    /**
     * @var integer $id
     */
    private $id;

    /**
     * @var integer $vulnerabilityId
     */
    private $vulnerabilityId;

    /**
     * @var integer $extReferenceTypeId
     */
    private $extReferenceTypeId;

    /**
     * @var text $value
     */
    private $value;

    /**
     * @var boolean $indirect
     */
    private $indirect;

    /**
     * @var boolean $fourofour
     */
    private $fourofour;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set vulnerabilityId
     *
     * @param integer $vulnerabilityId
     */
    public function setVulnerabilityId($vulnerabilityId)
    {
        $this->vulnerabilityId = $vulnerabilityId;
    }

    /**
     * Get vulnerabilityId
     *
     * @return integer 
     */
    public function getVulnerabilityId()
    {
        return $this->vulnerabilityId;
    }

    /**
     * Set extReferenceTypeId
     *
     * @param integer $extReferenceTypeId
     */
    public function setExtReferenceTypeId($extReferenceTypeId)
    {
        $this->extReferenceTypeId = $extReferenceTypeId;
    }

    /**
     * Get extReferenceTypeId
     *
     * @return integer 
     */
    public function getExtReferenceTypeId()
    {
        return $this->extReferenceTypeId;
    }

    /**
     * Set value
     *
     * @param text $value
     */
    public function setValue($value)
    {
        $this->value = $value;
    }

    /**
     * Get value
     *
     * @return text 
     */
    public function getValue()
    {
        return $this->value;
    }

    /**
     * Set indirect
     *
     * @param boolean $indirect
     */
    public function setIndirect($indirect)
    {
        $this->indirect = $indirect;
    }

    /**
     * Get indirect
     *
     * @return boolean 
     */
    public function getIndirect()
    {
        return $this->indirect;
    }

    /**
     * Set fourofour
     *
     * @param boolean $fourofour
     */
    public function setFourofour($fourofour)
    {
        $this->fourofour = $fourofour;
    }

    /**
     * Get fourofour
     *
     * @return boolean 
     */
    public function getFourofour()
    {
        return $this->fourofour;
    }
}