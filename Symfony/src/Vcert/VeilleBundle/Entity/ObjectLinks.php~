<?php

namespace Vcert\VeilleBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Vcert\VeilleBundle\Entity\ObjectLinks
 */
class ObjectLinks
{
    /**
     * @var integer $id
     */
    private $id;

    /**
     * @var integer $vulnerabilityId
     */
    private $vulnerabilityId;

    /**
     * @var integer $objectCorrelationId
     */
    private $objectCorrelationId;

    /**
     * @var integer $objectAffectTypeId
     */
    private $objectAffectTypeId;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set vulnerabilityId
     *
     * @param integer $vulnerabilityId
     */
    public function setVulnerabilityId($vulnerabilityId)
    {
        $this->vulnerabilityId = $vulnerabilityId;
    }

    /**
     * Get vulnerabilityId
     *
     * @return integer 
     */
    public function getVulnerabilityId()
    {
        return $this->vulnerabilityId;
    }

    /**
     * Set objectCorrelationId
     *
     * @param integer $objectCorrelationId
     */
    public function setObjectCorrelationId($objectCorrelationId)
    {
        $this->objectCorrelationId = $objectCorrelationId;
    }

    /**
     * Get objectCorrelationId
     *
     * @return integer 
     */
    public function getObjectCorrelationId()
    {
        return $this->objectCorrelationId;
    }

    /**
     * Set objectAffectTypeId
     *
     * @param integer $objectAffectTypeId
     */
    public function setObjectAffectTypeId($objectAffectTypeId)
    {
        $this->objectAffectTypeId = $objectAffectTypeId;
    }

    /**
     * Get objectAffectTypeId
     *
     * @return integer 
     */
    public function getObjectAffectTypeId()
    {
        return $this->objectAffectTypeId;
    }
}