     * Set cveId
     *
     * @param string $cveId
     */
    public function setCveId($cveId)
    {
        $this->cveId = $cveId;
    }

    /**
     * Get cveId
     *
     * @return string 
     */
    public function getCveId()
    {
        return $this->cveId;
    }

