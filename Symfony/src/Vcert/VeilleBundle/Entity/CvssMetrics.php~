<?php

namespace Vcert\VeilleBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Vcert\VeilleBundle\Entity\CvssMetrics
 */
class CvssMetrics
{
    /**
     * @var integer $id
     */
    private $id;

    /**
     * @var string $accessVector
     */
    private $accessVector;

    /**
     * @var string $accessComplexity
     */
    private $accessComplexity;

    /**
     * @var string $authentication
     */
    private $authentication;

    /**
     * @var string $confidentialityImpact
     */
    private $confidentialityImpact;

    /**
     * @var string $integrityImpact
     */
    private $integrityImpact;

    /**
     * @var string $availabilityImpact
     */
    private $availabilityImpact;

    /**
     * @var string $source
     */
    private $source;

    /**
     * @var string $cveId
     */
    private $cveId;

    /**
     * @var datetime $generatedOn
     */
    private $generatedOn;

    /**
     * @var float $score
     */
    private $score;

    /**
     * @var float $calculatedCvssBaseScore
     */
    private $calculatedCvssBaseScore;

    /**
     * @var integer $vulnerabilityId
     */
    private $vulnerabilityId;

    /**
     * @var datetime $createdAt
     */
    private $createdAt;

    /**
     * @var datetime $updatedAt
     */
    private $updatedAt;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set accessVector
     *
     * @param string $accessVector
     */
    public function setAccessVector($accessVector)
    {
        $this->accessVector = $accessVector;
    }

    /**
     * Get accessVector
     *
     * @return string 
     */
    public function getAccessVector()
    {
        return $this->accessVector;
    }

    /**
     * Set accessComplexity
     *
     * @param string $accessComplexity
     */
    public function setAccessComplexity($accessComplexity)
    {
        $this->accessComplexity = $accessComplexity;
    }

    /**
     * Get accessComplexity
     *
     * @return string 
     */
    public function getAccessComplexity()
    {
        return $this->accessComplexity;
    }

    /**
     * Set authentication
     *
     * @param string $authentication
     */
    public function setAuthentication($authentication)
    {
        $this->authentication = $authentication;
    }

    /**
     * Get authentication
     *
     * @return string 
     */
    public function getAuthentication()
    {
        return $this->authentication;
    }

    /**
     * Set confidentialityImpact
     *
     * @param string $confidentialityImpact
     */
    public function setConfidentialityImpact($confidentialityImpact)
    {
        $this->confidentialityImpact = $confidentialityImpact;
    }

    /**
     * Get confidentialityImpact
     *
     * @return string 
     */
    public function getConfidentialityImpact()
    {
        return $this->confidentialityImpact;
    }

    /**
     * Set integrityImpact
     *
     * @param string $integrityImpact
     */
    public function setIntegrityImpact($integrityImpact)
    {
        $this->integrityImpact = $integrityImpact;
    }

    /**
     * Get integrityImpact
     *
     * @return string 
     */
    public function getIntegrityImpact()
    {
        return $this->integrityImpact;
    }

    /**
     * Set availabilityImpact
     *
     * @param string $availabilityImpact
     */
    public function setAvailabilityImpact($availabilityImpact)
    {
        $this->availabilityImpact = $availabilityImpact;
    }

    /**
     * Get availabilityImpact
     *
     * @return string 
     */
    public function getAvailabilityImpact()
    {
        return $this->availabilityImpact;
    }

    /**
     * Set source
     *
     * @param string $source
     */
    public function setSource($source)
    {
        $this->source = $source;
    }

    /**
     * Get source
     *
     * @return string 
     */
    public function getSource()
    {
        return $this->source;
    }

