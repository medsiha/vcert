        $this->calculatedCvssBaseScore = $calculatedCvssBaseScore;
    }

    /**
     * Get calculatedCvssBaseScore
     *
     * @return float 
     */
    public function getCalculatedCvssBaseScore()
    {
        return $this->calculatedCvssBaseScore;
    }

