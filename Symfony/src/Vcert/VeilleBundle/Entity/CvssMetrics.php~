     * Set generatedOn
     *
     * @param datetime $generatedOn
     */
    public function setGeneratedOn($generatedOn)
    {
        $this->generatedOn = $generatedOn;
    }

    /**
     * Get generatedOn
     *
     * @return datetime 
     */
    public function getGeneratedOn()
