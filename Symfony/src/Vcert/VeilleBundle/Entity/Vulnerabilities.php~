<?php

namespace Vcert\VeilleBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Vcert\VeilleBundle\Entity\Vulnerabilities
 */
class Vulnerabilities
{
    /**
     * @var integer $id
     */
    private $id;

    /**
     * @var integer $osvdbId
     */
    private $osvdbId;

    /**
     * @var string $title
     */
    private $title;

    /**
     * @var datetime $disclosureDate
     */
    private $disclosureDate;

    /**
     * @var datetime $discoveryDate
     */
    private $discoveryDate;

    /**
     * @var datetime $osvdbCreateDate
     */
    private $osvdbCreateDate;

    /**
     * @var datetime $lastModifiedDate
     */
    private $lastModifiedDate;

    /**
     * @var datetime $exploitPublishDate
     */
    private $exploitPublishDate;

    /**
     * @var datetime $solutionDate
     */
    private $solutionDate;

    /**
     * @var text $description
     */
    private $description;

    /**
     * @var text $solution
     */
    private $solution;

    /**
     * @var text $tDescription
     */
    private $tDescription;

    /**
     * @var text $manualNotes
     */
    private $manualNotes;

    /**
     * @var text $shortDescription
     */
    private $shortDescription;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set osvdbId
     *
     * @param integer $osvdbId
     */
    public function setOsvdbId($osvdbId)
    {
        $this->osvdbId = $osvdbId;
    }

    /**
     * Get osvdbId
     *
     * @return integer 
     */
    public function getOsvdbId()
    {
        return $this->osvdbId;
    }

    /**
     * Set title
     *
     * @param string $title
     */
    public function setTitle($title)
    {
        $this->title = $title;
    }

    /**
     * Get title
     *
     * @return string 
     */
    public function getTitle()
    {
        return $this->title;
    }

    /**
     * Set disclosureDate
     *
     * @param datetime $disclosureDate
     */
    public function setDisclosureDate($disclosureDate)
    {
        $this->disclosureDate = $disclosureDate;
    }

    /**
     * Get disclosureDate
     *
     * @return datetime 
     */
    public function getDisclosureDate()
    {
        return $this->disclosureDate;
    }

    /**
     * Set discoveryDate
     *
     * @param datetime $discoveryDate
     */
    public function setDiscoveryDate($discoveryDate)
    {
        $this->discoveryDate = $discoveryDate;
    }

    /**
     * Get discoveryDate
     *
     * @return datetime 
     */
    public function getDiscoveryDate()
    {
        return $this->discoveryDate;
    }

    /**
     * Set osvdbCreateDate
     *
     * @param datetime $osvdbCreateDate
     */
    public function setOsvdbCreateDate($osvdbCreateDate)
    {
        $this->osvdbCreateDate = $osvdbCreateDate;
    }

    /**
     * Get osvdbCreateDate
     *
     * @return datetime 
     */
    public function getOsvdbCreateDate()
    {
        return $this->osvdbCreateDate;
    }

    /**
     * Set lastModifiedDate
     *
     * @param datetime $lastModifiedDate
     */
    public function setLastModifiedDate($lastModifiedDate)
    {
        $this->lastModifiedDate = $lastModifiedDate;
    }

    /**
     * Get lastModifiedDate
     *
     * @return datetime 
     */
    public function getLastModifiedDate()
    {
        return $this->lastModifiedDate;
    }

    /**
     * Set exploitPublishDate
     *
     * @param datetime $exploitPublishDate
     */
    public function setExploitPublishDate($exploitPublishDate)
    {
        $this->exploitPublishDate = $exploitPublishDate;
    }

    /**
     * Get exploitPublishDate
     *
     * @return datetime 
     */
    public function getExploitPublishDate()
    {
        return $this->exploitPublishDate;
    }

    /**
     * Set solutionDate
     *
     * @param datetime $solutionDate
     */
    public function setSolutionDate($solutionDate)
    {
        $this->solutionDate = $solutionDate;
    }

    /**
     * Get solutionDate
     *
     * @return datetime 
     */
    public function getSolutionDate()
    {
        return $this->solutionDate;
    }

    /**
     * Set description
     *
     * @param text $description
     */
    public function setDescription($description)
    {
        $this->description = $description;
    }

    /**
     * Get description
     *
     * @return text 
     */
    public function getDescription()
    {
        return $this->description;
    }

    /**
     * Set solution
     *
     * @param text $solution
     */
    public function setSolution($solution)
    {
        $this->solution = $solution;
    }

    /**
     * Get solution
     *
     * @return text 
     */
    public function getSolution()
    {
        return $this->solution;
    }

    /**
     * Set tDescription
     *
     * @param text $tDescription
     */
    public function setTDescription($tDescription)
    {
        $this->tDescription = $tDescription;
    }

    /**
     * Get tDescription
     *
     * @return text 
     */
    public function getTDescription()
    {
        return $this->tDescription;
    }

    /**
     * Set manualNotes
     *
     * @param text $manualNotes
     */
    public function setManualNotes($manualNotes)
    {
        $this->manualNotes = $manualNotes;
    }

    /**
     * Get manualNotes
     *
     * @return text 
     */
    public function getManualNotes()
    {
        return $this->manualNotes;
    }

    /**
     * Set shortDescription
     *
     * @param text $shortDescription
     */
    public function setShortDescription($shortDescription)
    {
        $this->shortDescription = $shortDescription;
    }

    /**
     * Get shortDescription
     *
     * @return text 
     */
    public function getShortDescription()
    {
        return $this->shortDescription;
    }
}