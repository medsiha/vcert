<?php

namespace Vcert\VeilleBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Vcert\VeilleBundle\Entity\Credits
 */
class Credits
{
    /**
     * @var integer $id
     */
    private $id;

    /**
     * @var integer $vulnerabilityId
     */
    private $vulnerabilityId;

    /**
     * @var integer $authorId
     */
    private $authorId;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set vulnerabilityId
     *
     * @param integer $vulnerabilityId
     */
    public function setVulnerabilityId($vulnerabilityId)
    {
        $this->vulnerabilityId = $vulnerabilityId;
    }

    /**
     * Get vulnerabilityId
     *
     * @return integer 
     */
    public function getVulnerabilityId()
    {
        return $this->vulnerabilityId;
    }

    /**
     * Set authorId
     *
     * @param integer $authorId
     */
    public function setAuthorId($authorId)
    {
        $this->authorId = $authorId;
    }

    /**
     * Get authorId
     *
     * @return integer 
     */
    public function getAuthorId()
    {
        return $this->authorId;
    }
}